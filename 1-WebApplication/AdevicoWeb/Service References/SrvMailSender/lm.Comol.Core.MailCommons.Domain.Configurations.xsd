    </xs:sequence>
  </xs:complexType>
  <xs:element name="SmtpServiceConfig" nillable="true" type="tns:SmtpServiceConfig" />
  <xs:complexType name="SmtpAuthenticationConfig">
    <xs:sequence>
      <xs:element minOccurs="0" name="AccountName" nillable="true" type="xs:string" />
