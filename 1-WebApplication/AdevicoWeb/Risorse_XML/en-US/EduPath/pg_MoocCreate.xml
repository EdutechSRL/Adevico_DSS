  <item name="Selector.Name.DisplayPolicy.ModalForAllByAvailability" html="false" placeholder="false">all in modal (IF possible)</item>
  <item name="Selector.Name.DisplayPolicy.InheritedByPath" html="false" placeholder="false">inherit from mooc</item>
