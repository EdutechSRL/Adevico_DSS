  <item name="LBcompletionPolicy_t.text" html="false" placeholder="false">Completion:</item>
  <item name="Selector.Name.CompletionPolicy.NoUpdateIfCompleted" html="false" placeholder="false">do not recalculate completed ones</item>
  <item name="Selector.Name.CompletionPolicy.UpdateOnlyIfBetter" html="false" placeholder="false">update if the result is better</item>
