<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="lm.Comol.Core.DomainModel" namespace="lm.Comol.Core.Dashboard.Domain">
	<class name="liteTileTagAssociation" table="DB_TileTagAssociation" lazy="false" optimistic-lock ="none">
    <id name="Id" column="Id" type="Int64" unsaved-value="0">
      <generator class="native"/>
    </id>
    <version name="TimeStamp" generated="always" unsaved-value="null" type="BinaryBlob">
      <column name="_Timestamp" not-null="false" sql-type="timestamp"/>
    </version>
    <property name="IdTile" type="Int64" column="IdTile" update="false" insert="false"/>
    <many-to-one name="Tag" class="lm.Comol.Core.Tag.Domain.liteTagItem" lazy="false" column="IdTag" foreign-key="IdTag" cascade="none" />
  
    <property name="Deleted" type="lm.Comol.Core.DomainModel.BaseStatusDeleted,lm.Comol.Core.DomainModel" column="_Deleted" />
  </class>
</hibernate-mapping>