<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="lm.Comol.Core.DomainModel" namespace="lm.Comol.Core.DomainModel">
	<class name="CommunityRelation" table="CommunityRelation" optimistic-lock="version" dynamic-update="true">
		<id name="Id" column="CMRL_ID" type="Int64">
			<generator class="native"/>
		</id>
    <version name="TimeStamp" generated="always" unsaved-value="null" type="BinaryBlob">
      <column name="_Timestamp" not-null="false" sql-type="timestamp"/>
    </version>
		<many-to-one name="Source" class="Community" column="CMRL_SourceID" foreign-key="CMRL_SourceID" cascade="none" />
		<many-to-one name="Destination" class="Community" column="CMRL_DestinationID" foreign-key="CMRL_DestinationID" cascade="none" />
		<property name="RelationType" type="Int32" column="CMRL_CRLT_ID" />
		<property name="AutoGenerated" type="Boolean" column="CMRL_AutoGenerated" />
	</class>
</hibernate-mapping>