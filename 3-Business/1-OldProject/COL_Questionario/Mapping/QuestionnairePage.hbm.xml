    <id name="Id" column="QSPG_Id" type="Int32" unsaved-value="0">
      <generator class="native"/>
    </id>
    <property name="FromQuestion" type="Int32" column="QSPG_DallaDomanda" />
    <property name="ToQuestion" type="Int32" column="QSPG_AllaDomanda"  />
    <property name="Number" type="Int32" column="QSPG_NumeroPagina"  />
    <property name="IdQuestionnaireTranslation" type="Int32" column="QSPG_QSML_Id"  />
    <property name="Name" type="String" column="QSPG_NomePagina"  />
    <property name="DisplayRandom" type="Boolean" column="QSPG_RandomOrdineDomande"  />
    <property name="Description" type="String" column="QSPG_Descrizione"  />
  </class>
</hibernate-mapping>